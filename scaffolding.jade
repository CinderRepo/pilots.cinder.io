template(name="cover")
  .cover(data-state=cover)
    .coverContent
      .coverMessage(data-state="{{coverState 'message'}}")
        .coverMessageIcon
          +clapperIcon
        .serverMessage(data-state=logoutErrorExists)
          = logoutError
        //-.title Logging In..
      .coverInfoWrapper
        .coverInfo(data-state="{{coverState 'signup'}}")
          .title Signup
          .serverMessage(data-state=serverErrorExists)
            = serverError
          +autoForm class="coverForm" id="signupForm" schema=Schema.signupFormSchema
            .coverFormInputWrapper.first(data-state="{{#if afFieldIsInvalid name='username'}}error{{/if}}")
              +afFieldInput class="coverFormInput" name="username" type="text" placeholder="Username" autocomplete="off" autocorrect="off" autocapitalize="off" spellcheck="false"
              .coverFormMessage(data-state="{{#if afFieldIsInvalid name='username'}}active{{/if}}")
                {{{afFieldMessage name="username"}}}
            .coverFormInputWrapper.second(data-state="{{#if afFieldIsInvalid name='email'}}error{{/if}}")
              +afFieldInput class="coverFormInput" name="email" type="email" placeholder="email@email.com" autocomplete="off" autocorrect="off" autocapitalize="off" spellcheck="false"
              .coverFormMessage(data-state="{{#if afFieldIsInvalid name='email'}}active{{/if}}")
                {{{afFieldMessage name="email"}}}
            .coverFormInputWrapper.third(data-state="{{#if afFieldIsInvalid name='password'}}error{{/if}}")
              +afFieldInput class="coverFormInput" name="password" type="password" placeholder="Password" autocomplete="off" autocorrect="off" autocapitalize="off" spellcheck="false"
              .coverFormMessage(data-state="{{#if afFieldIsInvalid name='password'}}active{{/if}}")
                {{{afFieldMessage name="password"}}}
            button.coverFormSubmit(type="submit")
      .coverInfoWrapper
        .coverInfo(data-state="{{coverState 'login'}}")
          .title Login
          .serverMessage(data-state=serverErrorExists)
            = serverError
          +autoForm class="coverForm" id="loginForm" schema=Schema.loginFormSchema
            .coverFormInputWrapper.first(data-state="{{#if afFieldIsInvalid name='usernameOrEmail'}}error{{/if}}")
              +afFieldInput class="coverFormInput" name="usernameOrEmail" type="text" placeholder="Username Or Email" autocomplete="off" autocorrect="off" autocapitalize="off" spellcheck="false"
              .coverFormMessage(data-state="{{#if afFieldIsInvalid name='usernameOrEmail'}}active{{/if}}")
                {{{afFieldMessage name="usernameOrEmail"}}}
            .coverFormInputWrapper.second(data-state="{{#if afFieldIsInvalid name='password'}}error{{/if}}")
              +afFieldInput class="coverFormInput" name="password" type="password" placeholder="Password" autocomplete="off" autocorrect="off" autocapitalize="off" spellcheck="false"
              .coverFormMessage(data-state="{{#if afFieldIsInvalid name='password'}}active{{/if}}")
                {{{afFieldMessage name="password"}}}
            button.coverFormSubmit(type="submit")


template(name="browse")
  .browse
    .title(data-state=focused) Cinder
    .tiles
      each projects
        +tile

template(name="tile")
  .tile.clickable(data-menu="view",data-property=_id,data-state="{{focused _id}}",data-href=_id)
    .preview(style=previewSrc)
    .title= title
    .subtitle= owner

template(name="contentAbout")
  //-.scroller
  .contentAbout
    .pane
      with currentContent
        .title.clickable(data-menu="edit",data-property="title",data-state="{{focused 'title'}}")= title
        .author(data-state=focused)= owner
        .description.clickable(data-menu="edit",data-property="body",data-state="{{focused 'body'}}")= body
    .pane
      | Image

