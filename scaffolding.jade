            button.inlineFormSubmit(type="submit")
      .pane.browse
        .title.clickable(data-state=focused) Creations
        .tiles
          each projects
            +tile

template(name="community")
  .community
    .currentTopic
      with currentTopic
        //-.section(data-state=focused)= username
          h1 INVALID? {{#if afFieldIsInvalid name='title'}}INVALID{{/if}}
          {{{afFieldMessage name="title"}}}
        if isEditable owner
          +editableTopic
        else
          +topic
      each childTopics
        if isEditable owner
          +editableTopic
        else
          +topic

    .topics
      each topics
        +listableTopic

template(name="topic")
  .topic.clickable(data-state="{{focused _id}}",data-menu="topic")
    .bounds(style="padding-left:{{position ancestors}}px")
      if title
        .title= title
      .avatar
      .author= subtitle
      .description
        {{{body}}}
  each topicComments
    with this
      if isEditable owner
        +editableTopic
      else
        +topic

template(name="editableTopic")
  .topic.clickable(data-state="{{focused _id}}",data-menu="topic")
    .bounds(style="padding-left:{{position ancestors}}px")
      if title
        +autoForm id="topicTitleForm" schema=Schema.topicTitleFormSchema validation="keyup"
          +afFieldInput class="title" name="title" type="text" placeholder="Title" autocomplete="off" autocorrect="off" autocapitalize="off" spellcheck="false" value=title
          button(type="submit")
          //-.coverFormInputWrapper.first(data-state="{{#if afFieldIsInvalid name='title'}}error{{/if}}")
          //-.coverFormMessage(data-state="{{#if afFieldIsInvalid name='title'}}active{{/if}}")
            {{{afFieldMessage name="title"}}}
        //-.title= title
      .avatar
      .author= subtitle
      .description.editor
        {{{body}}}
  each editableTopicComments
    with this
      if isEditable owner
        +editableTopic
      else
        +topic

template(name="listableTopic")
  .topic.clickable(data-state="{{focused _id}}",data-menu="listableTopic",data-href=_id)
    .title= title
    .avatar
    .author= subtitle
    .description
      {{{body}}}

